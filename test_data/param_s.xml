<?xml version="1.0"?>
<!-- minimal valid oscdoc instance -->
<!-- param_s, attributes, whitelist -->
<osc_unit format_version="1.0">
	<meta>
		<name>a_prog</name>
		<uri>http://unique.uri/for/that/prog/ending_with_slash/</uri>
		<doc_origin>http://where.to/?find=this&amp;xml_file</doc_origin>
	</meta>
	<message_in pattern="/a/path/in" typetag="s">
		<param_s symbol="s_i1" units="free-style units">
			<desc>optional desc 1</desc>
			<desc>optional desc 2</desc>
			<custom>c1</custom>
			<custom>c2</custom>
		</param_s>
	</message_in>
<!-- attributes, whitelist -->
	<message_in pattern="/b/path/in" typetag="s">
		<param_s symbol="s_i1" min_length="5">
			<whitelist>
				<token>a</token>
			</whitelist>
		</param_s>
	</message_in>
	<message_in pattern="/c/path/in" typetag="s">
		<param_s symbol="s_i1" max_length="20">
			<whitelist>
				<token>a a</token>
			</whitelist>
		</param_s>
	</message_in>
	<message_in pattern="/d/path/in" typetag="s">
		<param_s symbol="s_i1" regex="^test">
			<whitelist>
				<token>atoken</token>
				<token>btoken</token>
			</whitelist>
		</param_s>
	</message_in>
	<message_in pattern="/e/path/in" typetag="s">
		<param_s symbol="s_i1" min_length="5" max_length="20" regex="^test" units="special units" >
			<whitelist>
				<token>atoken</token>
				<token>btoken</token>
			</whitelist>
		</param_s>
	</message_in>
<!-- the same for out messages -->


	<message_out pattern="/a/path/out" typetag="s">
		<param_s symbol="s_i1" units="free-style units">
			<desc>optional desc 1</desc>
			<desc>optional desc 2</desc>
			<custom>c1</custom>
			<custom>c2</custom>
		</param_s>
	</message_out>
<!-- attributes, whitelist -->
	<message_out pattern="/b/path/out" typetag="s">
		<param_s symbol="s_i1" min_length="5">
			<whitelist>
				<token>a</token>
			</whitelist>
		</param_s>
	</message_out>
	<message_out pattern="/c/path/out" typetag="s">
		<param_s symbol="s_i1" max_length="20">
			<whitelist>
				<token>a a</token>
			</whitelist>
		</param_s>
	</message_out>
	<message_out pattern="/d/path/out" typetag="s">
		<param_s symbol="s_i1" regex="^test">
			<whitelist>
				<token>atoken</token>
				<token>btoken</token>
			</whitelist>
		</param_s>
	</message_out>
	<message_out pattern="/e/path/out" typetag="s">
		<param_s symbol="s_i1" min_length="5" max_length="20" regex="^test" units="special units" >
			<whitelist>
				<token>atoken</token>
				<token>btoken</token>
			</whitelist>
		</param_s>
	</message_out>
</osc_unit>

