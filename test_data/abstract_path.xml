<?xml version="1.0"?>
<!-- minimal valid oscdoc instance -->
<!-- abstract path -->
<osc_unit format_version="1.0">
	<meta>
		<name>a_prog</name>
		<uri>http://unique.uri/for/that/prog/ending_with_slash/</uri>
		<doc_origin>file://abstract_path.xml</doc_origin>
	</meta>

	<message_in pattern="/aaa[22,1]/[1,]/*/p.+ath[12,14]/aaa[4]" typetag="I">
		<param_I symbol="c_i1">
			<desc>optional desc 1</desc>
			<desc>optional desc 2</desc>
			<custom>c1</custom>
			<custom>c2</custom>
		</param_I>
	</message_in>
<!-- ranges -->
	<message_in pattern="/" typetag="I">
		<param_I symbol="c_i1"/>
	</message_in>

<!-- almost the same for out messages -->
	<message_out pattern="/[1,]b/path[2]/out[3,7]" typetag="I">
		<param_I symbol="c_i1">
			<desc>optional desc 1</desc>
			<desc>optional desc 2</desc>
			<custom>c1</custom>
			<custom>c2</custom>
		</param_I>
	</message_out>
	<message_out pattern="/c/path/out" typetag="I">
		<param_I symbol="s_i1"/>
	</message_out>

</osc_unit>

